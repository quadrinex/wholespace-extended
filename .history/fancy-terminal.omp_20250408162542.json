{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\u256d"
        },
        {
          "style": "plain",
          "template": " {{ if .WSL }}WSL > {{ end }}{{.Icon}}  ",
          "type": "os",
          "foreground": "lightBlue"
        },
        {
          "type": "shell",
          "style": "plain"
          "foreground": "lightBlue"
        },
        {
          "type": "sysinfo",
          "style": "plain",
          "template": "î¿… {{ round .PhysicalPercentUsed .Precision }}% ",
          "foreground": "#07e6b5"
        },
        {
          "type": "connection",
          "style": "plain",
          "template": "{{ if eq .Type \"wifi\"}}\uf1eb {{ .Name }} {{ end }}",
          "foreground": "#bd9b73"
        },
        {
          "properties": {
            "style": "austin",
            "always_enabled": true
          },
          "style": "plain", 
          "template": "{{ .FormattedMs }} ",
          "type": "executiontime",
          "foreground": "#e5e6b6"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFFFFF",
          "template": "\u2570"
        }
      ]
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "\ue285 "
  },
  "version": 3
}
